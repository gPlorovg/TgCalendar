DROP TABLE calendars, dates;
-- В таблице users пока нет необходимости
-- CREATE TABLE users
-- (
--     chat_id       BIGINT,
--     username      VARCHAR(32),
--     prefer_names  VARCHAR(64)[5]
-- );
CREATE TABLE marks
(
    event_id I       NTEGER,
    chat_id          BIGINT,
    positions        DATE[]
);
CREATE TABLE calendars
(
    group_id         BIGINT,
    event_id         BIGINT,
    event_name       VARCHAR(60),
    calendar_grid    VARCHAR(10)[35],
    active           BOOLEAN
);